{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "./src/*"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "degusta-box/auth": "dev-master",
        "degusta-box/core": "dev-master",
        "degusta-box/time-recording": "dev-master",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.13",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.3",
        "lexik/jwt-authentication-bundle": "^3.1",
        "nelmio/cors-bundle": "^2.5",
        "opis/json-schema": "^2.3",
        "stof/doctrine-extensions-bundle": "^1.12",
        "symfony/console": "7.1.*",
        "symfony/dotenv": "7.1.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "7.1.*",
        "symfony/messenger": "7.1.*",
        "symfony/runtime": "7.1.*",
        "symfony/security-bundle": "7.1.*",
        "symfony/uid": "7.1.*",
        "symfony/yaml": "7.1.*"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.2",
        "doctrine/doctrine-fixtures-bundle": "^3.6",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "7.1.*",
        "symfony/css-selector": "7.1.*",
        "symfony/maker-bundle": "^1.61",
        "symfony/phpunit-bridge": "^7.1"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "DegustaBox\\App\\": "src/App/",
            "DegustaBox\\Core\\": ["src/Core", "src/Core/src"],
            "DegustaBox\\Auth\\": ["src/Auth", "src/Auth/src"],
            "DegustaBox\\TimeRecording\\": ["src/TimeRecording", "src/TimeRecording/src"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DegustaBox\\App\\Tests\\": "src/App/Tests/",
            "DegustaBox\\Core\\Tests\\": "src/Core/Tests/",
            "DegustaBox\\Auth\\Tests\\": "src/Auth/Tests/",
            "DegustaBox\\TimeRecording\\Tests\\": "src/TimeRecording/Tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.1.*"
        }
    }
}
